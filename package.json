{
  "name": "vue-unit-testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"yarn serverless\" yarn \"vue-cli-service serve\"",
    "serverless": "cd serverless/nodejs-koa && now dev",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:factorial": "vue-cli-service test:unit ./tests/unit/factorial.spec.js",
    "test:list": "vue-cli-service test:unit ./src/store/list.spec.js",
    "test:my-component": "vue-cli-service test:unit ./src/components/MyComponent.spec.js",
    "test:clear-cache": "vue-cli-service test:unit --clearCache",
    "test:integration": "vue-cli-service test:unit --testMatch \"**/src/**/*.int.(js)\""
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-e2e-cypress": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
